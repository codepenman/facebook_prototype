<div ng-controller="RemoveMembersCtrl">
		<form>
			  <hr/>
			  <div class="form-group">
				  <label ng-if="membersAvailable">Select</label>
				  <li ng-repeat="member in groupMembers">
				  	<input type="checkbox" ng-change="onMemberSelect({{member}})" ng-model="friendSelected"></input><div class="hdivider-2"></div>{{member.name}}
				  </li>	  		  		 
			  </div>
		  	<button type="submit" class="btn btn-default pull-right" ng-click="removeFromGroup()">Remove</button>
		</form>
</div>